<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cua.domain.AdminInfo" table="admin_info" catalog="cua_test">
		<id name="adminId">
			<!-- <generator class="native"></generator> -->
		</id>
		<property name="adminNumber"></property>
		<property name="loginName"></property>
		<property name="adminName"></property>
		<property name="adminPassword"></property>
		<property name="adminDepartment"></property>
		<property name="adminSex"></property>
		<property name="adminMail"></property>
		<property name="adminPhoneNumber"></property>
		<property name="adminRole"></property>
	</class>
</hibernate-mapping>