<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cua.domain.JourneyInfo" table="journey_info" catalog="cua_test">
		<id name="journeyId">
			<generator class="native"></generator>
		</id>
		<property name="journeyDayNumber"></property>
		<property name="journeyTitle"></property>
		<property name="journeyDescription"></property>
		<property name="journeyFileName"></property>
		<property name="journeyRealName"></property>
		<property name="productName"></property>

	</class>
</hibernate-mapping>