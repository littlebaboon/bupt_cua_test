<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cua.domain.QuestionInfo" table="question_info" catalog="cua_test">
		<id name="questionId">
			<generator class="native"></generator>
		</id>
		<property name="questionName"></property>
		<property name="questionContent"></property>
		<property name="questionTime"></property>
		<property name="questionOrder"></property>

	</class>
</hibernate-mapping>