<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.cua.domain.TopPhotoInfo" table="topphoto_info" catalog="cua_test">
        <id name="topPhotoId">
            <generator class="native"></generator>
        </id>
        <property name="topPhotoFileName"></property>
        <property name="topPhotoRealName"></property>
        <property name="updateTime"></property>
        <property name="isTop"></property>
        <property name="topPhotoCity"></property>
    </class>
</hibernate-mapping>
