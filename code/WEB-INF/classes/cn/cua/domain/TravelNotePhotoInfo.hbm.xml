<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.cua.domain.TravelNotePhotoInfo" table="travelnotephoto_info" catalog="cua_test">
		<id name="travelNotePhotoId">
			<generator class="native"></generator>
		</id>
		<property name="travelNotePhotoFileName"></property>
		<property name="travelNotePhotoRealName"></property>
		<property name="travelNotePhotoAuthor"></property>
		<property name="travelNoteName"></property>

	</class>
</hibernate-mapping>